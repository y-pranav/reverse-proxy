{
  "server": {
    "port": 9999,
    "max_connections": 200,
    "connection_timeout": 15,
    "keep_alive": false
  },
  "logging": {
    "file": "reverse_proxy_least_conn.log",
    "level": "INFO",
    "console": true
  },
  "load_balancer": {
    "algorithm": "LEAST_CONNECTIONS",
    "backends": [
      {
        "host": "127.0.0.1",
        "port": 3000,
        "weight": 1,
        "enabled": true
      },
      {
        "host": "127.0.0.1",
        "port": 8000,
        "weight": 1,
        "enabled": true
      },
      {
        "host": "127.0.0.1",
        "port": 8080,
        "weight": 1,
        "enabled": true
      },
      {
        "host": "127.0.0.1",
        "port": 9000,
        "weight": 1,
        "enabled": true
      }
    ]
  },
  "health_check": {
    "enabled": false,
    "interval": 60,
    "path": "/status",
    "timeout": 10
  }
}
